<Window
	x:Class="APG_FEA2D.Views.NodalLoadView"
	xmlns="https://github.com/avaloniaui"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	xmlns:vm="using:APG_FEA2D.ViewModels"
	Title="NodalLoadView"
	Width="250"
	Height="150"
	d:DesignHeight="450"
	d:DesignWidth="800"
	WindowStartupLocation="CenterOwner"
	mc:Ignorable="d">
	<StackPanel x:DataType="vm:NodalLoadViewModel">
		<Label>Enter Rotation</Label>
		<TextBox Text="{Binding Rotation}">90.0</TextBox>
		<Label>Enter Load</Label>
		<TextBox Text="{Binding Load}">10</TextBox>
		<Button Command="{Binding NodalLoadOkCommand}">OK</Button>
	</StackPanel>
</Window>
